#address.tab-pane.fade.in.active.general-main-wrap
  = form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f|
    .hidden-xs.hidden-sm
      .row
        .col-md-5
          %h3.general-subtitle.mt-0= t('users.addresses.billing_address')
        .col-md-5.col-md-offset-1
          %h3.general-subtitle.mt-0 Shipping Address
    .row
      = render 'users/registrations/addresses/billing'
      = render 'users/registrations/addresses/shipping'